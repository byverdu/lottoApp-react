{
  "parser": "babel-eslint",
  "extends": "react-app",  
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "indent": [1, 2],
    "comma-dangle": [2, "never"],
    "react/jsx-indent-props": [2, 4],
    "space-in-parens": [2, "always", { "exceptions": ["()","{}","[]"] } ],
    "computed-property-spacing": [2, "always"]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  }
}